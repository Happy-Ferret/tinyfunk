{
  "name": "tinyfunk",
  "version": "0.0.0",
  "description": "The tiniest of functional libraries",
  "main": "tinyfunk.js",
  "author": "flintinatux",
  "license": "MIT",
  "scripts": {
    "build": "uglifyjs src.js -cm --toplevel > tinyfunk.js",
    "clean": "rm -f tinyfunk.js",
    "prewatch": "yarn run build",
    "test": "mocha --reporter=dot",
    "watch": "eye --*glob=src.js yarn run build"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eye": "^0.0.3",
    "mocha": "^3.5.3",
    "uglify-es": "^3.1.1"
  }
}
